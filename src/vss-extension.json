{
  "manifestVersion": 1,
  "extensionId": "vsts-swagger-diff",
  "name": "Swagger Contract Diff",
  "version": "1.2.2",
  "publisher": "knom",
  "description": "A build/release task for comparing Swagger Contracts.",
  "public": true,
  "galleryFlags": [],
  "icons": {
    "default": "images/logo.png",
    "large": "images/logo_large.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Swagger",
    "OpenAPI",
    "Build",
    "Release",
    "Compare",
    "OpenAPI",
    "REST",
    "WebAPI"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/knom/vsts-swagger-tasks/"
    },
    "issues": {
      "uri": "https://github.com/knom/vsts-swagger-tasks/issues"
    },
    "repository": {
      "uri": "https://github.com/knom/vsts-swagger-tasks/"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/knom/vsts-swagger-tasks/"
  },
  "branding": {
    "color": "#777",
    "theme": "dark"
  },
  "screenshots": [{
      "path": "docs/task.png"
    },
    {
      "path": "docs/result1.png"
    },
    {
      "path": "docs/result2.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "files": [{
      "path": "images",
      "addressable": true
    },
    {
      "path": "swagger-diff"
    }
  ],
  "targets": [{
    "id": "Microsoft.VisualStudio.Services"
  }],
  "contributions": [{
    "id": "swagger-diff",
    "type": "ms.vss-distributed-task.task",
    "targets": [
      "ms.vss-distributed-task.tasks"
    ],
    "properties": {
      "name": "swagger-diff"
    }
  }]
}